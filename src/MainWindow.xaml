<Window
    x:Class="TingenLieutenant.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TingenLieutenant"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Tingen Commander"
    Width="900"
    Height="500"
    Background="#f3e8d2"
    ResizeMode="NoResize"
    mc:Ignorable="d">
    <Grid>

        <!--  Tingen Commander logo  -->
        <Image
            x:Name="imgTingenLieutenantLogo"
            Width="184"
            Height="245"
            Margin="10,10,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Source="AppData/Image/Logo/TingenLieutenant_logo.png" />
    </Grid>

</Window>